package br.com.treinaweb.zoologico.classes;

public class Animal {
	private String nome;
	private int totalPatas;
	private int idade;
	
	
	public String getNome() {
		return this.nome;
	}
	
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public int getTotalPatas() {
		return this.totalPatas;
	}
	
	public void setTotalPatas(int totalPatas) {
		this.totalPatas = totalPatas;
	}
	
	public int getIdade() {
		return this.idade;
	}
	
	public void setIdade(int idade) {
		this.idade = idade;
	}	
	
	
 	Animal () {
		System.out.println("instancia de class Animal criada");
	}
}
