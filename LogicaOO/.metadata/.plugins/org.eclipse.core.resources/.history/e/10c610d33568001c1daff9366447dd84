package br.com.treinaweb.zoologico.classes;

import java.util.*;
import java.lang.reflect.*;


public class Main {
	
	public static void main(String args[]) {
		
		
		Animal a;
		Field[] allFields = Animal.class.getDeclaredFields();
		
		System.out.println(allFields.length);
		
		for(int row=0; row < 3; row++) {
			System.out.println(allFields[row]);
		}
		
	}
	
}