package br.com.treinaweb.zoologico.classes;

public class Animal {
	public String nome;
	public int total_patas;
	private int idade;
	
	
	public int getIdade() {
		return this.idade;
	}
	
	public void setIdade(int idade) {
		this.idade = idade;
	}	
	
	
 	Animal () {
		System.out.println("instancia de class Animal criada");
	}
}
